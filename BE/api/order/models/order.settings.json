{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "completed",
        "cancelled",
        "shipped"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "item": {
      "via": "order",
      "model": "item"
    }
  }
}
